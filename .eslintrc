{
  "extends": ["eslint:recommended", "prettier"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true,
    "jquery": true
  },
  "rules": {
    "indent": ["error", 2],
    "no-console": "off",
    "semi": [2, "always", { "omitLastInOneLineBlock": true }],
    "eol-last": ["error", "always"]
  }
}
